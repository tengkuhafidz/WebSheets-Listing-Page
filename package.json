    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.13.0",
    "eslint-plugin-react": "^7.19.0",
    "fuse.js": "^5.2.3",
    "gatsby": "^2.24.47",
    "gatsby-plugin-google-gtag": "^2.1.1",
    "gatsby-plugin-manifest": "^2.4.1",
    "gatsby-plugin-offline": "^3.2.0",
    "gatsby-plugin-postcss": "^2.3.0",
    "gatsby-plugin-react-helmet": "^3.3.0",
    "gatsby-plugin-typescript": "^2.4.0",
    "gatsby-source-google-sheets-flexible": "^1.0.0",
    "lint-staged": "^10.2.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.0.0",
    "typescript": "^3.8.3",
    "use-react-modal": "^0.0.9"
  },
  "devDependencies": {
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "eslint": "^5.14",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "tailwindcss": "^1.4.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tengkuhafidz/WebSheets-Listing-Page/"
  },
  "bugs": {
    "url": "https://github.com/tengkuhafidz/WebSheets-Listing-Page/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn lint:fix"
    ]
  }
